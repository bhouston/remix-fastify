    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
